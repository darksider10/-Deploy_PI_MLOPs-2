    "\n",
    "# Dividir la columna 'XY (CABA)' en dos nuevas columnas 'X_CABA' y 'Y_CABA'\n",
    "homicidios_hechos[['X_CABA', 'Y_CABA']] = homicidios_hechos['XY (CABA)'].str.split(' ', expand=True)"
   ]
